package br.com.alura.gerenciador.servlet;

public class Empresa {
	
	private Integer id;
	private String nome;
	
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	
	public void setName(String nomeEmpresa) {
		this.nome = nomeEmpresa;
	}
}
